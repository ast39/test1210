    "components": {
        "schemas": {
            "token": {
                "title": "Token data",
                "properties": {
                    "access_token": {
                        "type": "string",
                        "example": "abcdef1234"
                    },
                    "type": {
                        "type": "string",
                        "example": "Bearer"
                    },
                    "expires_in": {
                        "type": "integer",
                        "example": "3600"
                    }
                },
                "type": "object"
            },
            "error_response": {
                "title": "Error response",
                "properties": {
                    "error": {
                        "properties": {
                            "app": {
                                "type": "string",
                                "example": "Some Api"
                            },
                            "code": {
                                "type": "integer",
                                "example": "400"
                            },
                            "msg": {
                                "type": "string",
                                "example": "Bad request"
                            }
                        },
                        "type": "object"
                    }
                },
                "type": "object"
            },
            "user": {
                "title": "User data",
                "properties": {
                    "id": {
                        "type": "integer",
                        "example": "1"
                    },
                    "name": {
                        "type": "string",
                        "example": "Admin"
                    },
                    "email": {
                        "type": "string",
                        "example": "test@gmail.com"
                    },
                    "verified": {
                        "type": "string",
                        "format": "date-time",
                        "example": "null",
                        "nullable": "true"
                    },
                    "created": {
                        "type": "string",
                        "format": "date-time",
                        "example": "2023-10-04T16:33:11.000000Z"
                    },
                    "updated": {
                        "type": "string",
                        "format": "date-time",
                        "example": "2023-10-04T16:33:11.000000Z"
                    }
                },
                "type": "object"
            },
            "payment": {
                "title": "Payment data",
                "properties": {
                    "payment_id": {
                        "type": "integer",
                        "example": "1"
                    },
                    "curency": {
                        "type": "string",
                        "example": "USD"
                    },
                    "amount": {
                        "type": "numeric",
                        "format": "float",
                        "example": "50.50"
                    },
                    "amount_in_rub": {
                        "type": "numeric",
                        "format": "float",
                        "example": "5050"
                    },
                    "ticket": {
                        "type": "integer",
                        "example": "123456",
                        "nullable": "true"
                    },
                    "payer": {
                        "$ref": "#/components/schemas/user"
                    }
                },
                "type": "object"
            }
